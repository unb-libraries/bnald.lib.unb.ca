<?php

/**
 * @file
 * Install, update and uninstall for the default docker installation profile.
 */

/**
 * Implements hook_install().
 *
 * Perform actions to set up the site for this profile.
 *
 * @see system_install()
 */
function bnald_lib_unb_ca_install() {
  $config = \Drupal::service('config.factory')->getEditable('system.theme');
  $config->set('default', 'bartik')->save();
  $config->set('admin', 'seven')->save();
}
