{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of bnald.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/bnald.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/bootstrap": "3.23",
        "drupal/config_update": "1.5",
        "drupal/core": "8.9.3",
        "drupal/externalauth": "1.3",
        "drupal/features": "3.7",
        "drupal/google_analytics": "2.2",
        "drupal/health_check": "1.2",
        "drupal/ldap": "3.0-beta7",
        "drupal/noreqnewpass": "1.2",
        "drupal/pathauto": "1.8",
        "drupal/paragraphs": "1.5",
        "drupal/redis": "1.4",
        "drupal/search_api": "1.17",
        "drupal/search_api_autocomplete": "1.4",
        "drupal/search_api_solr": "4.1.6",
        "drupal/token": "1.7",
        "drupal/tvi": "1.0-rc1",
        "drupal/pdfpreview": "1.0",
        "drupal/migrate_tools": "4.1",
        "drupal/migrate_file": "1.1",
        "unb-libraries/drupal-builder": "^8.0.0",
        "drupal/migrate_source_csv": "^3.2"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
